{
  "title": "Open AWS Shortcut",
  "rules": [
    {
      "description": "Open AWS with numpad keys",
      "manipulators": [
        {
          "from": {
            "key_code": "keypad_1"
          },
          "to": [
            {
              "shell_command": "/usr/bin/open -a '/Applications/Google Chrome.app' 'https://ap-northeast-2.console.aws.amazon.com/console/home'"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "keypad_2"
          },
          "to": [
            {
              "shell_command": "/usr/bin/open -a '/Applications/Google Chrome.app' 'https://us-east-1.console.aws.amazon.com/cloudfront/v4/home'"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "keypad_3"
          },
          "to": [
            {
              "shell_command": "/usr/bin/open -a '/Applications/Google Chrome.app' 'https://ap-northeast-2.console.aws.amazon.com/codesuite/codepipeline/pipelines'"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "keypad_4"
          },
          "to": [
            {
              "shell_command": "/usr/bin/open -a '/Applications/Google Chrome.app' 'https://ap-northeast-2.console.aws.amazon.com/elasticbeanstalk/home'"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "keypad_5"
          },
          "to": [
            {
              "shell_command": "/usr/bin/open -a '/Applications/Google Chrome.app' 'https://ap-northeast-2.console.aws.amazon.com/cloudwatch/home'"
            }
